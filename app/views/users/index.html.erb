<div class="container-fluid">
<h1>Listing Users</h1>
<hr>
<ul class="nav nav-tabs">
  <li>
    <%= link_to "Articles", articles_path %>
    </li>
  <li class="active">
    <%= link_to "Users", users_path %>
  </li>
</ul>

<hr>

<div class="row">
 <table>
    <tr>
      <% @users.each do |user| %>
      <td><%= link_to 'See profile', user_path(user) %></td>
      <td>By: <%= user.username %></td>
      
    </tr>
  <% end %>
  
  
</table>
<%= will_paginate %>
</div>
</div> 